{
  "$schema": "https://schema.management.azure.com/schemas/2018-05-01/subscriptionDeploymentTemplate.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "resourceGroupName": {
      "type": "string",
      "defaultValue": "groot1229",
      "metadata": {
        "_parameterType": "resourceGroup",
        "description": "資源的資源群組名稱。建議您將資源放在相同的資源群組下，以改進追蹤。"
      }
    },
    "resourceGroupLocation": {
      "type": "string",
      "defaultValue": "japaneast",
      "metadata": {
        "_parameterType": "location",
        "description": "資源群組的位置。資源群組的位置可能與資源不同。"
      }
    },
    "resourceLocation": {
      "type": "string",
      "defaultValue": "[parameters('resourceGroupLocation')]",
      "metadata": {
        "_parameterType": "location",
        "description": "資源的位置。根據預設會使用資源群組的位置，除非該位置不支援資源提供者。"
      }
    }
  },
  "resources": [
    {
      "type": "Microsoft.Resources/resourceGroups",
      "name": "[parameters('resourceGroupName')]",
      "location": "[parameters('resourceGroupLocation')]",
      "apiVersion": "2019-10-01"
    },
    {
      "type": "Microsoft.Resources/deployments",
      "name": "[concat(parameters('resourceGroupName'), 'Deployment', uniqueString(concat('prjDBGamingFormShow', subscription().subscriptionId)))]",
      "resourceGroup": "[parameters('resourceGroupName')]",
      "apiVersion": "2019-10-01",
      "dependsOn": [
        "[parameters('resourceGroupName')]"
      ],
      "properties": {
        "mode": "Incremental",
        "template": {
          "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentTemplate.json#",
          "contentVersion": "1.0.0.0",
          "resources": [
            {
              "sku": {
                "name": "Free_F1",
                "tier": "Free",
                "size": "F1",
                "capacity": 1
              },
              "location": "[parameters('resourceLocation')]",
              "name": "prjDBGamingFormShow",
              "type": "Microsoft.SignalRService/SignalR",
              "apiVersion": "2018-10-01"
            }
          ]
        }
      }
    }
  ],
  "metadata": {
    "_dependencyType": "signalr.azure"
  }
}